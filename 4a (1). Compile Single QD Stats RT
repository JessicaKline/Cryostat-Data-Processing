 folder_string\n","\n","  t_avg = []\n","  beta = []\n","  g2_lst = []\n","  em_max = []\n","  fwhm = []\n","  on = []\n","  off = []\n","  grey = []\n","  bc_type = []\n","  auger_type = []\n","  num_lst = []\n","  total_counts_lst = []\n","  max_counts = []\n","  last_on_event = []\n","  num_levels = []\n","\n","  decay_rate = []\n","\n","\n","  for qd_num in range(0, 200):\n","    qd_num_path = directory + f'QD{qd_num}/'\n","    if os.path.exists(qd_num_path) == True:\n","      apd_loc_path = qd_num_path + 'APD/Analyzed Data/'\n","      spectra_loc_path = qd_num_path + 'Spectra/'\n","\n","      ## g2 values\n","      g2_val = open(apd_loc_path+\"g2_val.txt\", \"r\")\n","      g2_val = g2_val.read()\n","\n","      if float(g2_val) < 0.4:\n","        g2_lst.append(float(g2_val))\n","\n","        num_lst.append(qd_num)\n","\n","        ## average lifetime\n","        ## beta value\n","        QD_TRPL_Fit = pd.read_csv(apd_loc_path+\"QD_TRPL_Fit.csv\")\n","        QD_TRPL_Fit = QD_TRPL_Fit.to_numpy()\n","\n","        t_avg.append((QD_TRPL_Fit[0,1]/QD_TRPL_Fit[3,1])*gamma(1/QD_TRPL_Fit[3,1]))\n","        beta.append(QD_TRPL_Fit[3,1])\n","\n","\n","        ## count auger vs BC vs HC\n","        a_v_b = read_csv(apd_loc_path+\"FLID_fit_paras.csv\")\n","        bc_type.append(a_v_b[0][7])\n","        auger_type.append(1-a_v_b[0][7])\n","\n","\n","        ## ON vs Grey vs OFF\n","        ## ON defined as max intensity state\n","        ## OFF defined as min intensity state\n","        ## GREY are mixed states a and (1-a) ON and OFF respectively\n","        state_lst = read_csv(apd_loc_path+\"state_stats.csv\")\n","        max_counts.append(np.nanmax(state_lst[:,1]))\n","\n","        num_levels.append(len(state_lst[:,1]))\n","\n","        max_int = np.argmax(state_lst[:,1])\n","        min_int = np.argmin(state_lst[:,1])\n","\n","        state_lst[:,1][state_lst[:,1] < 300] = 300\n","        frac = (state_lst[:,1] - 300)/(state_lst[max_int, 1] - 300)\n","        off_temp = (1-frac)*state_lst[:,3]\n","        off.append(np.nansum(off_temp))\n","\n","        on_temp = frac*state_lst[:,3]\n","        on.append(np.nansum(on_temp))\n","        grey.append(0)\n","\n","\n","        ##time of last on event\n","        times = read_csv(apd_loc_path+'last_ON_event.csv')\n","        last_on_event.append(np.nanmax(times))\n","\n","        ## total intensity\n","        total_counts_val = open(apd_loc_path+\"total_counts.txt\", \"r\")\n","        total_counts_val = total_counts_val.read()\n","        total_counts_lst.append(float(total_counts_val))\n","\n","        ## emission max and FWHM\n","        pl_peak = read_csv(spectra_loc_path+f'fit_paras.csv')\n","        em_max.append(pl_peak[0,2])\n","        fwhm.append(pl_peak[0,3]*1000*2.355)\n","\n","\n","        ### decay rate\n","        decay_txt = open(apd_loc_path+\"decay_fit.txt\", \"r\")\n","        decay_txt = decay_txt.read()\n","        decay_txt = decay_txt.split()\n","        decay_para = [float(x) for x in decay_txt]\n","\n","        decay_rate.append(decay_para[0]*1000)\n","      # else:\n","      #   print(g2_val)\n","\n","\n","  print(directory)\n","  print(len(num_lst))\n","  total_arr = np.array([num_lst, em_max, fwhm, total_counts_lst, t_avg, beta, g2_lst, on, grey, off, bc_type, auger_type, max_counts, last_on_event, num_levels, decay_rate])\n","\n","  save_csv_report(total_arr, ['QD num', 'em max (ev)', 'fwhm (meV)', 'total counts',  't avg (ns)', 'beta', 'g2', 'ON', 'GREY', 'OFF', 'bc type', 'auger type', 'max cps', 'time last ON (s)',\n","                              'num_levels', 'decay rate (cps)'], directory +'Compiled_QD_Stats.csv')"]}],"metadata":{"colab":{"provenance":[],"authorship_tag":"ABX9TyPrMWEFfTYJ3niu3ajPMexj"},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}